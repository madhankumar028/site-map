{
  "name": "site-map",
  "version": "1.0.0",
  "description": "PickyourTrail sitemap",
  "main": "index.js",
  "scripts": {
    "build-css": "node-sass --include-path scss ./app.scss ./app.css",
    "watch-css": "nodemon -e scss -x \"npm run build-css\"",
