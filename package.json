    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-preset-es2015": "^6.24.1",
    "concurrently": "^3.5.0",
    "node-sass": "^4.13.1",
    "nodemon": "^1.12.1",
    "webpack": "^4.0.0",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.11"
  },
